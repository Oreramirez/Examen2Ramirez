<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ramirez.examen">
    <!---Establecer permisos -->

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="EXAMEN UNIDAD"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.ramirez.examen.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.ramirez.examen.DetalleNoticia"
            android:label="@string/title_activity_detalle_noticia"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>